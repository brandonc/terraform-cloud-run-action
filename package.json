{
  "name": "terraform-cloud-run-action",
  "version": "0.1.0",
  "description": "A GitHub Action that creates an apply or destroy run in a terraform cloud workspace.",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/action.ts -o dist --license licenses.txt",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "test": "jest --passWithNoTests",
    "prepare": "husky install"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "axios": "^1.3.3",
    "axios-retry": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@vercel/ncc": "^0.34.0",
    "husky": "^8.0.2",
    "jest": "^29.2.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "license": "MPL-2.0"
}
